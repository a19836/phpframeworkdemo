<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<sql_mapping>
	<class name="MenuObjectGroup" table="mmenu_object_group">
		<id column="group_id"></id>
		<id column="object_type_id"></id>
		<id column="object_id"></id>
	
		<relationships>
			<one_to_many name="menu_object_group">
				<attribute column="*" table="mmenu_group" />
				
				<key pcolumn="group_id" fcolumn="group_id" ftable="mmenu_group" />
			</one_to_many>
		</relationships>
	</class>
</sql_mapping>

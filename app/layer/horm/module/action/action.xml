<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<sql_mapping>
	<class name="Action" table="mact_action">
		<id column="action_id"></id>
	
		<relationships>
			<one_to_many name="user_actions">
				<attribute column="*" table="mact_user_action" />
				
				<key pcolumn="action_id" fcolumn="action_id" ftable="mact_user_action" />
			</one_to_many>
		</relationships>
	</class>
</sql_mapping>
